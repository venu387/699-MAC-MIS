<div class="modal fade" id="companyDetailModal" tabindex="-1"
	role="dialog" aria-labelledby="exampleModalLongTitle"
	aria-hidden="true">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="exampleModalLongTitle">Company
					Profile</h5>
				<button type="button" class="close" data-dismiss="modal"
					aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">
				<div class="row">
					<label for="example-text-input" class="col-3 col-form-label">Company
						Name:</label>
					<div class="col-9">
						<label class="col-form-label">${company.name}</label>
					</div>
				</div>
				<div class="row">
					<label for="example-text-input" class="col-3 col-form-label">Address:</label>
					<div class="col-9">
						<label class="col-form-label">${company.address}</label>
					</div>
				</div>
				<div class="row">
					<label for="example-text-input" class="col-3 col-form-label">City:</label>
					<div class="col-9">
						<label class="col-form-label">${company.city}</label>
					</div>
				</div>
				<div class="row">
					<label for="example-text-input" class="col-3 col-form-label">Province:</label>
					<div class="col-9">
						<label class="col-form-label">${company.province}</label>
					</div>
				</div>
				<div class="row">
					<label for="example-text-input" class="col-3 col-form-label">Postal
						Code:</label>
					<div class="col-9">
						<label class="col-form-label">${company.postalCode}</label>
					</div>
				</div>
				<div class="row">
					<label for="example-text-input" class="col-3 col-form-label">Country:</label>
					<div class="col-9">
						<label class="col-form-label">${company.country}</label>
					</div>
				</div>
				<div class="row">
					<label for="example-text-input" class="col-3 col-form-label">Contact
						First Name:</label>
					<div class="col-9">
						<label class="col-form-label">${company.contactFirstName}</label>
					</div>
				</div>
				<div class="row">
					<label for="example-text-input" class="col-3 col-form-label">Contact
						Last Name:</label>
					<div class="col-9">
						<label class="col-form-label">${company.contactLastName}</label>
					</div>
				</div>
				<div class="row">
					<label for="example-text-input" class="col-3 col-form-label">Contact
						Position:</label>
					<div class="col-9">
						<label class="col-form-label">${company.contactPosition}</label>
					</div>
				</div>
				<div class="row">
					<label for="example-text-input" class="col-3 col-form-label">Contact
						Email ID:</label>
					<div class="col-9">
						<label class="col-form-label">${company.contactEmailId}</label>
					</div>
				</div>
				<div class="row">
					<label for="example-text-input" class="col-3 col-form-label">Contact
						Telephone:</label>
					<div class="col-9">
						<label class="col-form-label">${company.contactTelephone}</label>
					</div>
				</div>
				<div class="row">
					<label for="example-text-input" class="col-3 col-form-label">Company
						Website:</label>
					<div class="col-9">
						<label class="col-form-label">${company.website}</label>
					</div>
				</div>
				<div class="row">
					<label for="example-text-input" class="col-3 col-form-label">Notes:</label>
					<div class="col-9">
						<label class="col-form-label">${company.notes}</label>
					</div>
				</div>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
			</div>
		</div>
	</div>
</div>